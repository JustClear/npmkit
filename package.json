{
  "name": "npmkit",
  "moduleName": "npmkit",
  "version": "1.0.0",
  "description": "npm starter.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=development rollup -w -c rollup.config.js",
    "docs": "rm -rf ./docs/docs && jsdoc -c .jsdoc.json --verbose",
    "dev": "NODE_ENV=development rollup -c rollup.config.js",
    "prod": "NODE_ENV=production rollup -c rollup.config.js",
    "build": "rm -rf dist && npm run dev && npm run prod"
  },
  "repository": {
    "url": "https://github.com/JustClear/npmkit.git",
    "type": "git"
  },
  "author": "大板栗 <576839360@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "better-docs": "^2.0.1",
    "jsdoc-babel": "^0.5.0",
    "rollup": "^2.15.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  }
